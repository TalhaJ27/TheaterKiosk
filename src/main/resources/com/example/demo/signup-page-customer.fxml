<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="481.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.Login_GUI_Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Pane fx:id="pane_main" prefHeight="283.0" prefWidth="600.0">
      <children>
         <ImageView fx:id="image_logo" fitHeight="54.0" fitWidth="149.0" layoutX="239.0" layoutY="63.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@logo1.png" />
            </image>
         </ImageView>
         <Label fx:id="label_email" layoutX="179.0" layoutY="106.0" text="Email">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
         <Label layoutX="179.0" layoutY="182.0" text="Password">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
         <PasswordField fx:id="tf_signup_password" layoutX="314.0" layoutY="180.0" opacity="0.43" />
         <TextField fx:id="tf_signup_email" layoutX="314.0" layoutY="104.0" />
          <Button fx:id="btn_signup" layoutX="245.0" layoutY="227.0" onAction="#onSignupConfirmClick" prefHeight="42.0" prefWidth="111.0" text="SignUp">
            <font>
               <Font size="19.0" />
            </font>
         </Button>
         <Label layoutX="180.0" layoutY="68.0" text="Name">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
         <TextField fx:id="tf_signup_name" layoutX="314.0" layoutY="66.0" />
         <TextField fx:id="tf_signup_phone" layoutX="314.0" layoutY="142.0" />
         <Label layoutX="179.0" layoutY="144.0" text="Phone">
            <font>
               <Font size="15.0" />
            </font>
         </Label>
      </children>
      <effect>
         <Glow />
      </effect>
   </Pane>
   <Hyperlink fx:id="link_login" onMouseClicked="#onLoginLinkClick" text="Sign In" />
</VBox>
